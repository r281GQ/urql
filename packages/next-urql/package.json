    "url": "https://github.com/FormidableLabs/urql.git",
    "directory": "packages/next-urql"
  },
  "main": "dist/next-urql.js",
  "module": "dist/next-urql.es.js",
  "types": "dist/types/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "LICENSE",
    "CHANGELOG.md",
    "README.md",
    "dist/"
